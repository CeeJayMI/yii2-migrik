{
    "name":              "insolita/yii2-migration-generator" ,
    "description":       "Set of gii tools for generating  files for migration by schema of table , phpdoc or table data" ,
    "type":              "yii2-extension" ,
    "keywords":          [
        "yii2" ,
        "extension"
    ] ,
    "license":           "MIT" ,
    "minimum-stability": "dev" ,
    "authors":           [
        {
            "name":  "insolita" ,
            "email": "donnainsolita@gmail.com"
        }
    ] ,
    "autoload":          {
        "psr-4": {
            "insolita\\migrik\\": "src"
        }
    } ,
    "autoload-dev":      {
        "psr-4": {
            "tests\\": "tests"
        }
    } ,
    "require":           {
        "yiisoft/yii2": ">=2.0.14"
    } ,
    "require-dev":       {
        "yiisoft/yii2-gii":          "*" ,
        "nesbot/carbon":             "*" ,
        "codeception/verify":        "1.3.0" ,
        "codeception/specify":       "1.3.0",
        "phpunit/phpunit":           "^6.0|^7.0|^8.0"
    } ,
    "extra":             {
        "bootstrap": "insolita\\migrik\\Bootstrap"
    }
}
